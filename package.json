{
  "name": "vault-pam-ui",
  "description": "Vault Privileged Access Management: a custom enhanced Vault client.",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:mountaingapsolutions/vault-pam-ui.git"
  },
  "dependencies": {
