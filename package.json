    "crypto-js": "^3.1.9-1",
    "dateformat": "^3.0.3",
    "dotenv": "^8.0.0",
    "dotenv-expand": "5.1.0",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "express-session": "^1.16.1",
    "hcl-to-json": "^0.1.1",
    "hsts": "^2.2.0",
    "md5": "^2.2.1",
    "nodemailer": "^6.2.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "pm2": "^3.5.1",
    "request": "^2.88.0",
    "sequelize": "^5.8.7",
    "socket.io": "^2.2.0",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.5",
    "umzug": "^2.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@loadable/component": "^5.10.1",
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@svgr/webpack": "^4.3.0",
    "archiver": "^3.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "bfj": "^6.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.9.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "3.0.1",
    "fs-extra": "8.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.8.0",
    "jest-watch-typeahead": "^0.3.1",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.4.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "^4.0.1",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "resolve": "1.11.0",
    "sass-loader": "^7.1.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "validator": "^11.0.0",
    "warning": "^4.0.3",
    "webpack": "^4.32.2",
    "webpack-dev-server": "^3.5.1",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "optionalDependencies": {
    "vault-pam-premium": "bitbucket:mountaingapsolutions/vault-pam-premium"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "startprod": "node productionServer",
    "prebuild": "rm -rf dist && rm -f vault-pam-ui.zip",
    "build": "GENERATE_SOURCEMAP=false node scripts/build.js",
    "postbuild": "mkdir dist && mv build dist/build && npm run copyappfiles && node scripts/dist.js",
    "copyappfiles": "cp .env dist/ || : && cp package.json dist/ && cp robots.txt dist/ && cp src/productionServer.js dist/ && cp -R src/services dist",
    "installpremium": "npm install bitbucket:mountaingapsolutions/vault-pam-premium --no-save && echo Successfully installed premium features. || echo Failed to install. Premium features will not be available.",
    "lint": "eslint --ext .js,.jsx --format table .",
    "test": "node scripts/test.js",
    "startpm2prod": "pm2-runtime start productionServer.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not op_mini all",
    "not ie 11",
    "not kaios > 0",
    "not samsung > 0"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx,ts,tsx}",
      "src/services/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
